<h4><%= link_to "Browse Genres", genres_path %></h4>

<div class="index-container">
<% @movies.each do |movie| %>
  <div class="poster-div">
  <%= link_to movie_reviews_path(movie) do %>
    <img src="<%=movie.poster_url%>" class="index-posters">
  <% end %>
  </div>
<% end %>
</div>

<%= paginate @movies %>
